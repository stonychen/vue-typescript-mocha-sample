{
  "name": "vue-typescript-mochapack-sample",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/stonychen/vue-typescript-mocha-sample.git",
  "scripts": {
    "serve": "NODE_ENV=development webpack-dev-server --open --config ./build/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "analyse": "NODE_ENV=production webpack --config ./build/webpack.report.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts,.tsx src tests",
    "server": "NODE_ENV=development node server/index.js",
    "test": "NODE_ENV=production mochapack --webpack-config ./build/webpack.test.js --require tests/setup.ts tests/unit/**/*.spec.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "bl": "browserslist",
    "v-serve": "vue-cli-service serve",
    "v-build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "async-validator": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chromedriver": "83",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^26.1.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "less-loader": "^6.2.0",
    "mocha": "^8.0.1",
    "mochapack": "^2.0.2",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "~3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
